HOME		= /root/
RANDFILE	= $ENV::HOME/.rnd

[ req ]
prompt	= no
default_bits	= 2048
default_md		= sha256
encrypt_key 	= yes
distinguished_name	= req_dn
x509_extensions 	= cert_type

[req_dn]
C=US
ST=Ohio
L=Anytown
O=rootwyrm/TaleCaster
OU=TaleCaster/Headphones
CN=_REPLACE_HOSTNAME_
emailAddress=postmaster@_DOMAIN_

[cert_type]
nsCertType	= server
nsComment	= "TaleCaster/Headphones Auto-Generated Certificate"
